#include <iostream>
using namespace std;

int main() {
    int value[] = {60, 100, 120};  // profit of each item
    int weight[] = {10, 20, 30};   // weight of each item
    int W = 50;                    // capacity of knapsack
    int n = 3;                     // number of items

    int dp[n + 1][W + 1];          // DP table

    // Build DP table
    for (int i = 0; i <= n; i++) {
        for (int w = 0; w <= W; w++) {
            if (i == 0 || w == 0)
                dp[i][w] = 0;
            else if (weight[i - 1] <= w)
                dp[i][w] = max(value[i - 1] + dp[i - 1][w - weight[i - 1]], dp[i - 1][w]);
            else
                dp[i][w] = dp[i - 1][w];
        }
    }

    cout << "Maximum profit = " << dp[n][W];
    return 0;
}
